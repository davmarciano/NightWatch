<h1>My Movies</h1>
<hr>
<div class="row" style="justify-content: normal;">
  <% @mymovies.each do |movie|  %>
    <%= link_to movie_path(movie), class: "movie-card-link" do  %>
    <div class="movie-card">
      <img src="<%= movie.poster %>" class="card-img-top" alt="Movie Poster">
      <p> <%= movie.title %> </p>
    </div>
    <% end %>
  <% end %>
</div>
